  <!DOCTYPE html>
  <html>
    <head>
      <!--Import Google Icon Font-->
      <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
      <!--Import materialize.css-->
      <link type="text/css" rel="stylesheet" href="/css/materialize.min.css"  media="screen,projection"/>
      <script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.6.4/angular.min.js"></script>
      <!--Let browser know website is optimized for mobile-->
      <meta name="viewport" content="width=device-width, initial-scale=1.0"/>
    </head>

    <body ng-app = "myApp" class="card-panel red lighten-4">

        <div class="container">

                <div class="row"  ng-controller = "studentController">

                          <table>
                            <thead>
                              <tr>
                                  <th>Resturent Name</th>
                                  <th>Phonenumber</th>
                                  <th>Long and Lat</th>
                                  <th>Wait Time</th>

                              </tr>
                            </thead>

                            <tbody>
                              <tr ng-repeat = "resturent in resturents">

                              <td>{{ resturent.name }}</td>
                              <td>{{ resturent.rating }}</td>
                              <td>{{ resturent.latitude }}, {{ resturent.longitude }}</td>
                              <td>{{ resturent.waittime }}</td>

                              </tr>
                            </tbody>
                          </table>


                </div>

        </div>

      <!--Import jQuery before materialize.js-->
      <script type="text/javascript" src="https://code.jquery.com/jquery-3.2.1.min.js"></script>
      <script type="text/javascript" src="/js/materialize.min.js"></script>

      <script type="text/javascript">

var myApp = angular.module("myApp", []);

(function(app){
  "use strict";
  app.controller("studentController", function($scope, $http){
    $http.get('http://localhost:3000/api/resturents').then(function(prd){
      $scope.resturents = prd.data.results;

      console.log(prd.data.results);
    });
  });
})(myApp);


      </script>

    </body>
  </html>